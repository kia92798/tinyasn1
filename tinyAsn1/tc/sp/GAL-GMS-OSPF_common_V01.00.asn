-- GSS Status Data
GssStatusData                ::= SEQUENCE {
 
  
  -- GSTtimeStamp Time corresponding to generation of gssStatus and
  -- pre-processing statistics pstGssPreproResults as well as
  -- Gss clock stability information GssClkAllanDev.
  generationTime          GSTtimeStamp,
  
  -- GSS Sanity and Rej. Status Information
  gssInfo                 SEQUENCE (SIZE(0..max-GSS))
                            OF GssStatusStatistics
}
    
-- GSS Sanity and Rej. Status Information
GssStatusStatistics     ::= SEQUENCE {
    
  -- Time where data time interval for each GSS begins
  initialTime             GSTtimeStamp,
  
  -- Time where data time interval for each GSS begins
  endTime                 GSTtimeStamp,
  
  -- GSS Identifier 
  gssId                   GAL-GMS-GSS-GLAdType,
  
  -- Total Num. of Meas. in Sanity Status Stats.
