-- Ephemeris
-- Ephemeris Parameters
Ephemeris                     ::= SEQUENCE {

  -- Mean anomaly at reference time  [Semi-circles]
  m0                                INTEGER (-2147483648..2147483647),

  -- Mean motiondifference from computed value  [Semi-circles]
  dn                                INTEGER (-32768..32767),

  -- Eccentricity  [N/A]
  e                                 INTEGER (0..4294967295),

  -- Square root of the semi-major axis  [m^(1/2)]
  aSqRoot                           INTEGER (0..4294967295),

  -- Longitude of ascending node weekly epoch of orbital plane at  
  -- [Semi-circles] (Right ascension of the ascending node at reference time)
  omega0                            INTEGER (-2147483648..2147483647),

  -- Inclination angle at reference time  [Semi-circles]
  i0                                INTEGER (-2147483648..2147483647),

  -- Argument of perigee  [Semi-circles]
  omega                             INTEGER (-2147483648..2147483647),

  -- Rate of right ascension  [Semi-circles/s]
  omegaDot                          INTEGER (-8388608..8388607),

  -- Rate of inclination angle  [Semi-circles/s]
  iDot                              INTEGER (-8192..8191),

  -- Amplitude of the cosine harmonic to the argument of latitude  [rad]
  cuc                               INTEGER (-32768..32767),

  -- Amplitude of the sine harmonic correction term to the argument of 
  -- latitude 16 *  [rad] 
  cus                               INTEGER (-32768..32767),

  -- Amplitude of the cosine harmonic to the orbit radius correction term  [m]
  crc                               INTEGER (-32768..32767),

  -- Amplitude of the sine the orbit radius harmonic correction term to  [m]
  crs                               INTEGER (-32768..32767),

  -- Amplitude of the cosine harmonic to the angle of inclination correction 
  -- term  [rad] 
  cic                               INTEGER (-32768..32767),

  -- Amplitude of the sine harmonic correction term to the angle of 
  -- inclination  [rad] 
  cis                               INTEGER (-32768..32767),

  -- Ephemeris reference time  [s]
  t0e                               INTEGER (0..16383)
}

-- Almanac-Parameters
-- Almanac Parameters
Almanac-Parameters            ::= SEQUENCE {

  -- Square root of the absolute value of the deviation of the mean 
  -- semi-major axis from its nominal value [m^(1/2)] 
  aSqRoot                           INTEGER (-65536..65535),

  -- Eccentricity
  ecc                               INTEGER (0..2047),

  -- Inclination at reference time relative to i0 =56 deg [Semi-circles]
  deltai                            INTEGER (-1024..1023),

  -- Right ascension [Semi-circles]
  omega0                            INTEGER (-32768..32767),

  -- Rate of right ascension [Semi-circles/s]
  omegaDot                          INTEGER (-1024..1023),

  -- Argument of perigee [Semi-circles]
  w                                 INTEGER (-32768..32767),

  -- Satellite mean anomaly at reference time [Semi-circles]
  m0                                INTEGER (-32768..32767),

  -- Satellite clock correction bias "truncated" [s]
  af0                               INTEGER (-8192..8191),

  -- Satellite clock correction linear "truncated" [s/s]
  af1                               INTEGER (-1024..1023),

  -- Almanac Issue Of Data 
  iod                               INTEGER (0..3),

  -- Almanac reference time [s]
  t0a                               INTEGER (0..255),

  -- Almanac reference Week Number [weeks]
  wna                               INTEGER (0..255)
}

