-- Message definitions for interface EFT and Testport
-- ============================================================================
  
-- ClkBias
-- Clock offset at a given epoch and for a given clock referred to GST
ClkBias                       ::= SEQUENCE {

  -- Estimated clock offset for the corresponding epoch with respect to GSTtimeStamp 
  -- Units: cm  
  dBias                             DOUBLE,

  -- Status of the clock estimation for each satellite and epoch
  --  (0: NOK, 1: OK)
  bStatus                           BOOLEAN
}

-- ClockJumps
-- Satellite and GSS clock jumps at a given epoch.
ClockJumps                    ::= SEQUENCE {

  -- Gss clock jumps at a given epoch and service
  ptGssJumps                        SEQUENCE (SIZE(max-GSS))
                                     OF ClockJumpPar,

  -- Sat clock jumps at a given epoch and service
  pstSatJumps                       SEQUENCE (SIZE(max-SV-ID))
                                     OF ClockJumpPar
}

-- ClockJumpPar
-- Parameters that describe a clock jump
ClockJumpPar                  ::= SEQUENCE {

  -- Flag to indicate the existance of a clock jump.
  -- Units: N/A
  -- (1: Clock Jump)
  bJump                             BOOLEAN,

  -- Estimated clock jump magnitude. The clock jump is positive when 
  -- the pseudorange and carrier phase measurements are increased:
  -- (RHOafter-jump = (RHObefore-jump + Magnitude)
  --  (PHIafter-jump = (PHIbefore-jump + Magnitude)
  -- Units: metre
  dMagnitude                        DOUBLE
}

-- ManFields
-- ODTS parameters for describing a detected manoeuvre.
ManFields                     ::= SEQUENCE {

  -- Manoeuvre time in MJD format and in TAI reference
  stMjdTaiTime                      GSTtimeStamp,

  -- Estimated position of the CoM after manoeuvre in local satellite 
  -- reference frame (X, Y and Z) Units: meters 
  pdNewPos                          SEQUENCE (SIZE(dim-VECTOR))
                                     OF DOUBLE,

  -- Estimated values for the components of the velocity increment 
  -- associated to the manoeuvre, in the satellite orbital frame (walong, 
  -- wcross , wrad). Units: m/s 
  pdDeltaV                          SEQUENCE (SIZE(dim-VECTOR))
                                     OF DOUBLE,

  -- New mass after satellite manoeuvre Units: Kg
  dNewMass                          DOUBLE,

  -- Status of the estimated satellite manoeuvre.  (0: NOK, 1: OK)
  -- Units: N/A
  bStatus                           BOOLEAN
}

-- SrpCoeff
-- Solar Radiation Pressure Coefficients and status for one satellite
SrpCoeff                      ::= SEQUENCE {

  -- Value of the Solar Radiation Pressure model estimated parameters for 
  -- each satellite (in principle, Cr and Y-bias) and the considered service 
  -- Units: N/A 
  pdCoeff                           SEQUENCE (SIZE(num-EST-SRP))
                                     OF DOUBLE,

  -- Status of estimated SRP coefficients from ODTS for each satellite for 
  -- the considered service.  (0: NOK, 1: OK)
  -- Units: N/A 
  bStatus                           BOOLEAN
}

-- SteeringParam
-- Clock steering parameters
SteeringParam ::= SEQUENCE {

  -- GSSclock bias for the prescribed time window
  -- Range:
  -- Units: s
  dA0                           REAL,

  -- GSS clock drift for the prescribed time window
  -- Range:
  -- Units: s/s
  dA1                           REAL,

  -- GSS clock frequency drift for the prescribed time window
  -- Range:
  -- Units: s/s2
  dA2                           REAL,

  -- Reference epoch of the clock model
  -- Range:
  -- Units:
  stTocEpoch                    GSTtimeStamp,

  -- Time of applicability of the clock models.
  -- Range:
  -- Units: seconds
  siTimeOfApplicability         INTEGER,
 
  -- Status of the provided clock model  (0: NOK, 1: OK)
  -- Range: TRUE | FALSE
  -- Units: N/A
  bClockModStatus               BOOLEAN
}

