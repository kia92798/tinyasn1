MY-MODULE DEFINITIONS  EXPLICIT TAGS  ::=
BEGIN
EXPORTS
	MySQ0,
	A,
	B;


    ---- no tag glass although a is optional (it is tagged)

    MySQ0 ::= SEQUENCE {
        a [2] EXPLICIT A OPTIONAL,
        b B
    }

    A ::= CHOICE {
        d [0] EXPLICIT NULL,
        e [1] EXPLICIT NULL
    }

    B ::= CHOICE {
        d [0] EXPLICIT NULL,
        e [1] EXPLICIT NULL
    }



END
