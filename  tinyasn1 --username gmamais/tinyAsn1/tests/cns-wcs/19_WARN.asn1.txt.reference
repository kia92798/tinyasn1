MY-MODULE DEFINITIONS AUTOMATIC TAGS  ::=
BEGIN
EXPORTS
	MYSQ,
	MYSQ2,
	a1,
	b2;


	MYSQ ::= SEQUENCE {
		a [0] IMPLICIT INTEGER  (1..10),
		b [1] IMPLICIT INTEGER  OPTIONAL,
		c [2] IMPLICIT INTEGER  DEFAULT 4
	}

	MYSQ2 ::= MYSQ (WITH COMPONENTS {..., b (1..10) PRESENT})


	a1 MYSQ ::= { a 10 }
	b2 MYSQ2 ::= { a 10 }

END
