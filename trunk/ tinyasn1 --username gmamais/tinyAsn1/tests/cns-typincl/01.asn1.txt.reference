MY-MODULE DEFINITIONS AUTOMATIC TAGS  ::=
BEGIN
EXPORTS
	Day,
	Weekend,
	FrenchWeekend,
	LongWeekend,
	holiday;


	Day ::= ENUMERATED {
		monday(1),
		tuesday(2),
		wednesday(3),
		thursday(4),
		friday(5),
		saturday(6),
		sunday(7)
	}

	Weekend ::= Day ((saturday(6)|sunday(7)))

	FrenchWeekend ::= Day (Weekend)

	LongWeekend ::= Day ((Weekend|friday(5)))


	holiday LongWeekend ::= sunday(7)

END
